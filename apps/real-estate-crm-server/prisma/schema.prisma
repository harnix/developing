datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Client {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model Property {
  address     String?
  createdAt   DateTime            @default(now())
  description String?
  id          String              @id @default(cuid())
  price       Float?
  size        Int?
  status      EnumPropertyStatus?
  updatedAt   DateTime            @updatedAt
}

model AgentAssignment {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model Appointment {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

enum EnumPropertyStatus {
  Option1
}
